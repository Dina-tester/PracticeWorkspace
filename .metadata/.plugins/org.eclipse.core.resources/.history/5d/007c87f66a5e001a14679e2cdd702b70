package org.in;

public class Demo {
public static void main(String[] args) {
	int arr[] = {1, 2, 2, 3, 4, 4, 4, 5, 5};
	int n= arr.length;
	//int temp[]=null;
	
	for (int i = 0; i < n-1; i++) {
		int j = 0;
		if(arr[i]!=arr[i+1])
		{
		 arr[j]=arr[i];
		 j++;
		}
		System.out.println(arr[i]);
	}
	//arr[j]=arr[n-1];
}

}