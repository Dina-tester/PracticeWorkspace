package org.in;

public class Demo {
public static void main(String[] args) {
	int arr[] = {1, 2, 2, 3, 4, 4, 4, 5, 5};
	int n= arr.length;
	int temp[]=null;
	int j = 0;
	for (int i = 0; i < n; i++) {
		if(arr[i]!=arr[i+1])
		{
		 temp[j]=arr[i];
		 j++;
		}
	}
	temp[j]=arr[n-1];
	System.out.println(temp[j]);
}

}